<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="#288CFF"
    android:padding="8dp"
    tools:context=".MainActivity_NO">

    <Button
        android:id="@+id/btn_move_BJ2"
        android:layout_width="111dp"
        android:layout_height="103dp"
        android:layout_marginEnd="50dp"
        android:layout_marginBottom="50dp"
        android:backgroundTint="#0288FF"
        app:icon="@drawable/baseline_accessible_24"
        app:iconTint="#000000"
        app:layout_constraintBottom_toTopOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/btnNext"
        android:layout_width="90dp"
        android:layout_height="100dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="2dp"
        android:layout_marginBottom="50dp"
        android:backgroundTint="#DEE2E4"
        app:icon="@drawable/baseline_arrow_forward_24"
        app:iconTint="#000000"
        app:layout_constraintBottom_toTopOf="@+id/textView9"
        app:layout_constraintStart_toEndOf="@+id/button5"
        app:layout_constraintTop_toBottomOf="@+id/rcMain"
        app:layout_constraintVertical_bias="1.0" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rc1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/tedure1white_left_manus"
        app:layout_constraintBottom_toBottomOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="@+id/function_text"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/function_text" />


    <TextView
        android:id="@+id/function_text"
        android:layout_width="0dp"
        android:layout_height="100dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:background="#000000"
        android:text="송원대"
        android:textColor="#FFFFFF"
        android:textSize="80dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="214dp"
        android:layout_height="100dp"
        android:background="@drawable/tedure1white_left_manus"
        android:text="곧도착:"
        android:textColor="#000000"
        android:textSize="60dp"
        app:layout_constraintStart_toStartOf="@+id/function_text"
        app:layout_constraintTop_toBottomOf="@+id/function_text" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="250dp"
        android:layout_height="100dp"
        android:background="@drawable/tedure1white_left_manus"
        android:text="승차예약:"
        android:textColor="#000000"
        android:textSize="60dp"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rc2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/tedure1white_left_manus"
        app:layout_constraintBottom_toBottomOf="@+id/textView4"
        app:layout_constraintEnd_toEndOf="@+id/rc1"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/rc1" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="202dp"
        android:layout_height="100dp"
        android:layout_marginStart="8dp"
        android:background="@drawable/tedure_yellow"
        android:gravity="center"
        android:text="노선번호"
        android:textColor="#000000"
        android:textSize="50dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="202dp"
        android:layout_height="100dp"
        android:background="@drawable/tedure"
        android:gravity="center"
        android:text="혼잡도"
        android:textColor="#000000"
        android:textSize="50dp"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toTopOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="165dp"
        android:layout_height="100dp"
        android:background="@drawable/tedure"
        android:gravity="center"
        android:text="승차벨"
        android:textColor="#000000"
        android:textSize="50dp"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toTopOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:background="@drawable/tedure"
        android:gravity="center"
        android:text="도착정보"
        android:textColor="#000000"
        android:textSize="50dp"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="@+id/textView5" />

    <Button
        android:id="@+id/button5"
        android:layout_width="90dp"
        android:layout_height="100dp"
        android:layout_marginStart="248dp"
        android:layout_marginBottom="48dp"
        android:backgroundTint="#DEE2E4"
        app:icon="@drawable/baseline_arrow_back_24"
        app:iconTint="#000000"
        app:layout_constraintBottom_toTopOf="@+id/textView9"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        android:text="현재 모드는 모든 노선 버스가 표기 됩니다."
        android:textColor="#000000"
        android:textSize="35dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.7875" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rcMain"
        android:layout_width="765dp"
        android:layout_height="461dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.542"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1617"
        android:textColor="#FFFFFF"
        android:textSize="40sp"
        app:layout_constraintBottom_toBottomOf="@+id/function_text"
        app:layout_constraintEnd_toEndOf="@+id/function_text"
        app:layout_constraintHorizontal_bias="0.347"
        app:layout_constraintStart_toStartOf="@+id/function_text"
        app:layout_constraintTop_toTopOf="@+id/function_text"
        app:layout_constraintVertical_bias="1.0" />


</androidx.constraintlayout.widget.ConstraintLayout>